    <script is:inline>
      const themeToggle = document.getElementById("theme-toggle");
      const html = document.documentElement;

      const savedTheme = localStorage.getItem("theme") || "light";
      html.classList.toggle("dark", savedTheme === "dark");

      themeToggle.addEventListener("click", () => {
        html.classList.toggle("dark");
        const isDark = html.classList.contains("dark");
        localStorage.setItem("theme", isDark ? "dark" : "light");
      });

      const downloadPdfButton = document.getElementById("download-pdf");

      downloadPdfButton.addEventListener("click", () => {
        const wasDark = html.classList.contains("dark");
        if (wasDark) {
          html.classList.remove("dark");
        }

        setTimeout(() => {
          window.print();

          setTimeout(() => {
            if (wasDark) {
              html.classList.add("dark");
            }
          }, 1000);
        }, 100);
      });

      window.addEventListener("beforeprint", () => {
        document.title = "Mohamed Osman - Resume";
      });

      window.addEventListener("afterprint", () => {
        document.title = "Mohamed Osman — Full Stack Engineer";
      });

      const viewTranscriptButton = document.getElementById("view-transcript");

      viewTranscriptButton.addEventListener("click", () => {
        const pdfUrl = "f967cb21-c842-4f94-bad1-022dc66851ac (1).pdf";
        const popup = window.open(
          pdfUrl,
          "transcript",
          "width=900,height=700,scrollbars=yes,resizable=yes,toolbar=no,menubar=no,location=no,status=no"
        );

        if (!popup) {
          alert("Please allow popups for this site to view the transcript.");
        }
      });

      document.querySelectorAll('a[href^="#"]').forEach(anchor => {
        anchor.addEventListener('click', function (e) {
          e.preventDefault();
          const target = document.querySelector(this.getAttribute('href'));
          if (target) {
            target.scrollIntoView({
              behavior: 'smooth',
              block: 'start'
            });
          }
        });
      });

      const sections = document.querySelectorAll('section[id]');
      const navDots = document.querySelectorAll('nav a');

      window.addEventListener('scroll', () => {
        let current = '';
        sections.forEach(section => {
          const sectionTop = section.offsetTop;
          if (scrollY >= sectionTop - 200) {
            current = section.getAttribute('id');
          }
        });

        navDots.forEach(dot => {
          const span = dot.querySelector('span:first-child');
          if (dot.getAttribute('href') === `#${current}`) {
            span.classList.add('bg-accent');
            span.classList.remove('bg-ink-lighter', 'dark:bg-ink-dark-light');
          } else {
            span.classList.remove('bg-accent');
            span.classList.add('bg-ink-lighter', 'dark:bg-ink-dark-light');
          }
        });
      });
    </script>

